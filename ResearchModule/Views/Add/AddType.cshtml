@model List<string>

@{
    if (Model.Count() == 0)
    {
        <div class="label-text">Укажите вид публикации</div>
        <div>
            <input type="text" class="form-control" name="SectionName" />
        </div>
    }
    else
    {
        <div class="dropdown">
            <input type="text" class="form-control dropdown-toggle" id="searchType" placeholder="Введите вид публикации" data-toggle="dropdown" onkeyup="RM.SearchInput('searchAuthor','@(Url.Action("GetType", "Add"))')" />
        </div>
    }

}
