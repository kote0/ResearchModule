@page
@model ResearchModule.Pages.IndexModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<h1>Разделы</h1>
<table class="table">
    <thead>
        <tr>
            <th>№</th>
            <th>Название раздела</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Section)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.SectionName</td>
            </tr>
        }
    </tbody>
</table>

